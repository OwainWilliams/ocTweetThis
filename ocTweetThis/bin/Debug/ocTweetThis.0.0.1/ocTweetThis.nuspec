    <repository type="git" url="https://github.com/OwainWilliams/ocTweetThis" />
    <dependencies>
      <group targetFramework="net6.0">
        <dependency id="TweetinviAPI" version="5.0.4" exclude="Build,Analyzers" />
        <dependency id="Umbraco.Cms.Web.BackOffice" version="9.3.0" exclude="Build,Analyzers" />
        <dependency id="Umbraco.Cms.Web.Website" version="9.3.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
</package>